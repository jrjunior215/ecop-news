    box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);
  }

  #news-image {
    max-width: 100%;
    border-radius: 10px;
    margin-bottom: 20px;
  }

  .header_news {
    margin-bottom: 10px;
  }

  #news-title {
    font-size: 24px;
    font-weight: bold;
    color: #333;
  }

  #news-date {
    color: #888;
  }

  .desc_news {
    line-height: 1.6;
  }

  .news-info {
    margin-top: 20px;
    border-top: 1px solid #ddd;
    padding-top: 20px;
  }

  .news-info p {
    margin: 10px 0;
  }

  .news-info strong {
    font-weight: bold;
    margin-right: 5px;
  }

  .news-info span {
    color: #555;
  }
</style>

